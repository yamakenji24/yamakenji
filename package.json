{
  "name": "yamakenji",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^20.4.0",
    "npm": ">= 9.5.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "run-s build:posts build:works build:next",
    "build:posts": "ts-node --project tsconfig.builder.json ./src/builder/posts.ts",
    "build:works": "ts-node --project tsconfig.builder.json ./src/builder/works.ts",
    "build:next": "next build",
    "start": "next start",
    "prettier:quick": "pretty-quick --staged",
    "lint": "next lint --ignore-path .gitignore --dir src",
    "lint:fix": "next lint --ignore-path .gitignore --fix",
    "fmt": "prettier --write --ignore-path .gitignore 'src/**/*.{ts,tsx}'",
    "test": "jest",
    "test:upd": "jest --updateSnapshot",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "type-check": "tsc",
    "prepare": "run-s husky:install prepare:panda",
    "husky:install": "husky install",
    "prepare:panda": "panda codegen",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^2.3.4",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@vercel/og": "^0.0.20",
    "axios": "^0.27.2",
    "framer-motion": "^4.1.17",
    "next": "^13.4.6",
    "open-graph-scraper": "^4.9.2",
    "playwright-aws-lambda": "^0.7.0",
    "playwright-core": "^1.15.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-github-calendar": "^4.0.1",
    "react-icons": "^4.3.1",
    "react-tooltip": "^5.16.1"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.3.1",
    "@testing-library/cypress": "^8.0.1",
    "@testing-library/react": "^13.4.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.1.1",
    "@types/node": "^14.17.21",
    "@types/open-graph-scraper": "^4.8.1",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "autoprefixer": "^10.4.13",
    "cypress": "^8.5.0",
    "eslint": "^7.9.0",
    "eslint-config-next": "^13.4.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "fs-extra": "^10.0.0",
    "husky": "^8.0.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "rss-parser": "^3.12.0",
    "tailwindcss": "^3.2.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:fix",
      "npm run fmt"
    ]
  }
}
