    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-react": "^7.21.2",
    "husky": "^4.3.0",
    "lint-staged": "^11.2.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "typescript": "^4.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint 'src/**/*.{ts,tsx}' --fix"
    ]
