    "@testing-library/cypress": "^8.0.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/node": "^14.11.2",
    "axios": "^0.21.1",
    "cypress": "^8.5.0",
    "framer-motion": "^4.1.17",
    "jest": "^27.2.5",
    "next": "^11.1.2",
    "open-graph-scraper": "^4.9.2",
    "playwright-aws-lambda": "^0.7.0",
    "playwright-core": "^1.15.2",
    "react": "^17.0.1",
